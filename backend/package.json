{
   "name": "church-evangelism-backend",
   "version": "1.0.0",
   "description": "Backend for Church Evangelism Tracking System",
   "main": "dist/server.js",
   "scripts": {
      "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
      "build": "tsc",
      "start": "node dist/server.js",
      "db:migrate": "prisma migrate dev",
      "db:generate": "prisma generate",
      "db:seed": "ts-node prisma/seed.ts"
   },
   "dependencies": {
      "@apollo/server": "^4.7.1",
      "@prisma/client": "^5.0.0",
      "bcryptjs": "^2.4.3",
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "graphql": "^16.6.0",
      "graphql-scalars": "^1.24.2",
      "jsonwebtoken": "^9.0.0",
      "nexus": "^1.3.0"
   },
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "@types/jsonwebtoken": "^9.0.2",
      "@types/node": "^18.16.3",
      "prisma": "^5.0.0",
      "ts-node": "^10.9.1",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.0.4"
   }
}
